@model ACMWeb.Models.GradeItem

@{
    ViewBag.Title = "Upload";
}

<h2>Upload Grade Items</h2>
<form action="" method="post" enctype="multipart/form-data">
    <div class="form-group">
        @Html.LabelFor(model => model.GradeDistributionID, "GradeDistributionID", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.DropDownList("GradeDistributionID", null, htmlAttributes: new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.GradeDistributionID, "", new { @class = "text-danger" })
        </div>
    </div>
    <label for="file">Filename:</label>
    <input type="file" name="file" id="file" />

    <input type="submit" />
</form>
